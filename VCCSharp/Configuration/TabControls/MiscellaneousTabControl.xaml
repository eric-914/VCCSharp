<UserControl x:Class="VCCSharp.Configuration.TabControls.MiscellaneousTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:configuration="clr-namespace:VCCSharp.Configuration"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
  <UserControl.DataContext>
    <configuration:ConfigurationViewModel/>
  </UserControl.DataContext>
  <StackPanel Orientation="Vertical">
    <Label Content="Miscellaneous" />
    <CheckBox IsChecked="{Binding AutoStart}">AutoStart Emulation</CheckBox>
    <CheckBox IsChecked="{Binding CartAutoStart}">AutoStart Cart</CheckBox>
  </StackPanel>
</UserControl>
