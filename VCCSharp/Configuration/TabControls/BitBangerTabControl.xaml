<UserControl x:Class="VCCSharp.Configuration.TabControls.BitBangerTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VCCSharp.Configuration"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Vertical">
      <TextBox>Sample File Browse Text</TextBox>
      <StackPanel Orientation="Horizontal">
        <Button>Open</Button>
        <Button>Close</Button>
      </StackPanel>
      <CheckBox>Add LF to CR</CheckBox>
      <CheckBox>Print Monitor Window</CheckBox>
    </StackPanel>
</UserControl>
